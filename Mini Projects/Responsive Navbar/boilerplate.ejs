</head>
<body>

    <!-- <- include('navbar') %> -->

    <nav class="navbar navbar-expand-md bg-body-light border-bottom sticky-top">
        <div class="container-fluid">
          <a class="navbar-brand" href="/listings"><i class="fa-regular fa-compass"></i></a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>

          
          <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
    
            <div class="navbar-nav">
              <a class="nav-link" href="/listings">Explore </a>
            </div>
    
            <div class="navbar-nav ms-auto" >
              <form class="form-inline nav-search">
                <input class="form-control mr-sm-2" type="search" id="nav-search-input" placeholder="Search Destinations" aria-label="Search" disabled>
                <button class="btn btn-outline-success my-2 my-sm-0" id="nav-search-btn" type="submit" disabled><i class="fa-solid fa-magnifying-glass"></i>Search</button>
              </form>
            </div>
            
            <div class="navbar-nav ms-auto">
              <a class="nav-link" href="/listings/new">Add your Home</a>
                <a class="nav-link btn btn-dark nav-btn" href="/signup">Sign up</a>
                <a class="nav-link btn btn-dark nav-btn" href="/login">Log in </a>
                <!-- <a class="nav-link btn btn-dark nav-btn" href="/logout">Log out</a> -->

               
            </div>
    
          </div>
        </div>
    </nav>

    <div id="filters" class="mb-2 ">
      <div class="filter">
          <div><i class="fa-solid fa-fire"></i></div>
          <p>Trending</p>
      </div>
      <div class="filter">
          <div><i class="fa-solid fa-house"></i></div>
          <p>Cabins</p>
      </div>
      <div class="filter">
          <div><i class="fa-solid fa-umbrella-beach"></i></div>
          <p>Beachfront</p>
      </div>
      <div class="filter">
          <div><i class="fa-solid fa-tractor"></i></div>
          <p>Farm</p>
      </div>
      <div class="filter">
          <div><i class="fa-solid fa-warehouse"></i></div>
          <p>Mansions</p>
      </div>
      <div class="filter">
          <div><i class="fa-solid fa-water"></i></div>
          <p>Lake</p>
      </div>
      <div class="filter">
          <div><i class="fa-solid fa-ship"></i></div>
          <p>Housboats</p>
      </div>
      <div class="filter">
          <div><i class="fa-solid fa-city"></i></div>
          <p>Top Cities</p>
      </div>
      <div class="filter">
          <div><i class="fa-solid fa-snowflake"></i></div>
          <p>Arctic</p>
      </div>

      <div id="tax-switch" class="ms-auto">
          <div class="form-check-reverse form-switch">
              <label class="form-check-label" for="flexSwitchCheckDefault">Total price with Tax</label>
              <input class="form-check-input" type="checkbox" role="switch" id="flexSwitchCheckDefault">
          </div>
      </div>
      
  </div>
    
